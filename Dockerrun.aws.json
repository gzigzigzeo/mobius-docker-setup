{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "core",
      "image": "670461063650.dkr.ecr.us-west-1.amazonaws.com/mobius/core",
      "memory": 512,
      "essential": true,
      "mountPoints": [
        {
            "containerPath": "/var/stellar-core",
            "sourceVolume": "stellar-core-data"
        },
        {
            "containerPath": "/var/log/stellar-core",
            "sourceVolume": "stellar-core-log"
        }
      ],
      "portMappings": [
          {
              "containerPort": 11625,
              "hostPort": 11625
          },
          {
              "containerPort": 11626,
              "hostPort": 11626
          }
      ]
    }
  ],
    "volumes": [
        {
            "host": {
                "sourcePath": "./.data/stellar-core"
            },
            "name": "stellar-core-data"
        },
        {
            "host": {
                "sourcePath": "./.log/stellar-core"
            },
            "name": "stellar-core-log"
        },
    ]
}
